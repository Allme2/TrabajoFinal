     <header class="main-header4">
          <div class="background-overlay text-white py-5">
                <div class="row mt-5 justify-content-center">
                        <div class="col-6 ">
                        <h5 class="text-center">Contacto</h5>
                    <form method="post" action="/contacto">

                        <div class="form-group">
                            <label for="nombre">Nombre :</label>
                            <input autofocus type="text" id="nombre" placeholder="Martin" name="nombre" class="form-control" required>

                        </div>
                        <div class="form-group">
                            <label for="correo">Correo :</label>
                            <input type="email" id="correo" placeholder="foo@example.com" class="form-control" required name="correo">
                        </div>
                        <div class="form-group">
                            <textarea class="form-control" name="comentario" placeholder="comentario"></textarea>
                        </div>
                        <button type="submit" class="btn btn-success btn-block">Enviar</button>

                        {{#if status}}
                        <h5 class="text-success text-center">{{message}}</h5>
                        {{else}}
                        <h5 class="text-danger text-center">{{message}}</h5>
                        {{/if}}
                    </form>
                </div>
            </div>
          </div>
      </header>